<h1>Sign in</h1>

<form action="<%= session_url %>" method="post">
  <input
    type="hidden"
    name="authenticity_token"
    value="<%= form_authenticity_token %>">

  <label>Email
    <input
      type="text"
      name="user[email]">
  </label>
  <br>

  <label>Password
    <input type="password" name="user[password]" id="user_password">
  </label>
  <br>

  <input type="submit">

</form>